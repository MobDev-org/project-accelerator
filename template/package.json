{
  "name": "projectaccelerator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "": "",
    "clean-cache": "watchman watch-del-all &&  rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "reinstall-node-modules": "rm -rf node_modules && rm yarn.lock  && yarn",
    "reinstall-clean-ios": "cd ios && rm Podfile.lock && rm -rf Pods && rm -rf OnSiteConcept.xcworkspace && cd .. && npx pod-install && yarn ios",
    "reinstall-clean-android": "cd android && ./gradlew clean && cd .. && adb reverse tcp:8081 tcp:8081 && yarn android",
    "clean-reinstall-all": "yarn clean-cache && yarn reinstall-node-modules && yarn reinstall-clean-android && yarn reinstall-clean-ios ",
    "-": "",
    "gen": "node generate",
    "gen:screen": "npx hygen screen new",
    "gen:slice": "npx hygen slice new"
  },
